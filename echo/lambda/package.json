    "@typescript-eslint/eslint-plugin": "^5.47.1",
    "@typescript-eslint/parser": "^5.47.1",
    "@vercel/ncc": "0.36.0",
    "body-parser": "^1.20.0",
    "eslint": "^8.30.0",
    "eslint-plugin-prettier": "4.2.1",
    "express": "^4.18.2",
    "jest": "^29.3",
    "jest-mock": "^29.3.1",
    "nock": "^13.2.9",
